{
  "formatVersion": 3,
  "display-name": "${mod_name}",
  "id": "${mod_id}",
  "version": "${mod_version}",
  "description": "${mod_desc}.",
  "authors": [ "YOU :D" ],
  "meta": {
    "icon": "${mod_id}:icons/icon.png"
  },
  "entrypoints": {
    "postInit": [
      "org.example.exmod.ExampleModPostinit"
    ],
    "init": [
      "org.example.exmod.ExampleMod"
    ],
    "preInit": [
      "org.example.exmod.ExampleModPreinit"
    ],
    "client-init": [
      "org.example.exmod.ClientInitializer"
    ]
  },
  "depends": {
    "cosmic-reach": ">=${cosmic_reach}",
    "puzzle-loader-cosmic": ">=${puzzle_loader_cosmic}",
    "puzzle-loader-core": ">=${puzzle_loader_core}",
    "cosmic-api": ">=${cosmic_api}"
  },
  "mixins": [
    "${mod_id}.mixins.json",
    {
      "config": "${mod_id}.client.mixins.json",
      "environment": "client"
    }
  ],
  "access-writers": [
    "${mod_id}.manipulator"
  ]
}